<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_inherit_form" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.view</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']" position="before">
                <field name="max_line_sequence" invisible="1"/>
            </xpath>
<!--             <xpath expr="//field[@name='order_line']" position="attributes">
                <attribute name="context">{'default_line_item': max_line_sequence, 'sale_id': active_id}</attribute>
            </xpath> -->
        </field>
    </record>
</odoo>
